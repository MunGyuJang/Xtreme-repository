    "\n",
    "string = []\n",
    "number = []\n",
    "\n",
    "for i in input_:\n",
    "    try:\n",
    "        number.append(int(i))\n",
    "    except:\n",
    "        string.append(i)\n",
    "\n",
    "result = ''.join(sorted(string) + [str(sum(number))])\n",
    "print(result)"
