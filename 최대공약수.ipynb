    "def gcd(array):\n",
    "    arr = array.copy()\n",
    "    while True:\n",
    "        x, y = arr[-2], arr[-1]\n",
    "        while (y):\n",
    "            x, y = y, x%y\n",
    "        arr[-2:] = x, \n",
    "        if len(arr) == 1:\n",
    "            return arr[0]\n",
    "\n",
    "gcd([49, 777, 164157])"
   ]
